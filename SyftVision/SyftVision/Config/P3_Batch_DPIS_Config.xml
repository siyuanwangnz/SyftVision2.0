				<Components>
					<Component reagent="O-" product="16" max="0.05"/>
					<Component reagent="OH-" product="17" max="0.05"/>
					<Component reagent="O2-" product="32" max="0.05"/>
					<Component reagent="NO2-" product="46" max="0.05"/>
					<Component reagent="NO3-" product="62" max="0.05"/>
					<Component reagent="H3O+" product="19" max="0.05"/>
					<Component reagent="NO+" product="30" max="0.05"/>
					<Component reagent="O2+" product="32" max="0.05"/>
				</Components>
			</SubChart>
			<SubChart name="PosWet Reagent Ions">
				<Mark main="Long Term" sub="Pos Wet"/>
				<Components>
					<Component reagent="H3O+" product="19"/>
					<Component reagent="NO+" product="30"/>
					<Component reagent="O2+" product="32"/>
				</Components>
			</SubChart>
			<SubChart name="PosWet Product Ions">
				<Mark main="Long Term" sub="Pos Wet"/>
				<Components>
					<Component reagent="H3O+" product="79"/>
					<Component reagent="H3O+" product="93"/>
					<Component reagent="NO+" product="92"/>
					<Component reagent="O2+" product="28"/>
					<Component reagent="O2+" product="78"/>
					<Component reagent="O2+" product="92"/>
					<Component reagent="O2+" product="150"/>
					<Component reagent="O2+" product="236"/>
				</Components>
			</SubChart>
			<SubChart name="NegWet Reagent Ions">
				<Mark main="Long Term" sub="Neg Wet"/>
				<Components>
					<Component reagent="OH-" product="17"/>
					<Component reagent="O2-" product="32"/>
				</Components>
			</SubChart>
			<SubChart name="NegWet Product Ions">
				<Mark main="Long Term" sub="Neg Wet"/>
				<Components>
					<Component reagent="OH-" product="91"/>
					<Component reagent="OH-" product="233"/>
					<Component reagent="O2-" product="124"/>
					<Component reagent="O2-" product="236"/>
				</Components>
			</SubChart>
			<SubChart name="NegDry Reagent Ions">
				<Mark main="Long Term" sub="Neg Dry"/>
				<Components>
					<Component reagent="O-" product="16"/>
					<Component reagent="NO2-" product="46"/>
					<Component reagent="NO3-" product="62"/>
				</Components>
			</SubChart>
			<SubChart name="NegDry Product Ions">
				<Mark main="Long Term" sub="Neg Dry"/>
				<Components>
					<Component reagent="O-" product="183"/>
					<Component reagent="O-" product="233"/>
				</Components>
			</SubChart>
		</Chart>

		<Chart name="Short Term Stability">
			<SubChart name="Reagent Ions RSD" phase="Sample" accept="Under">
				<Mark main="Short Term" sub="Pos Wet"/>
				<Mark main="Short Term" sub="Neg Wet"/>
