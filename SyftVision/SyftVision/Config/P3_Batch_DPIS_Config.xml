<?xml version="1.0" encoding="utf-8"?>
<Config>
	<Methods>
		<Method name="Reset Quadrupoles"/>
		<Method name="carrier gas"/>
		<Method name="dws pressure validation"/>
		<Method name="Sample Flows"/>
		<Method name="ups scanline calibration"/>
		<Method name="dws scanline calibration"/>
		<Method name="detector verification"/>
		<Method name="Detector Linearity Validation"/>
		<Method name="Syft (+-) Standard Validation Ultra">
			<Mark main="SSV" sub="Three Phase"/>
		</Method>
		<Method name="Background"/>
		<Method name="Injection pos">
			<Mark main="Injection" sub="Pos Wet"/>
		</Method>
		<Method name="Injection neg wet">
			<Mark main="Injection" sub="Neg Wet"/>
		</Method>
		<Method name="Injection neg dry">
			<Mark main="Injection" sub="Neg Dry"/>
		</Method>
		<Method name="Ultra +- performance check">
			<Mark main="Performance Check" sub="Three Phase"/>
		</Method>

		<Group repeat="7">
			<Method name="SIM pos calibrant">
				<Mark main="Long Term" sub="Pos Wet"/>
			</Method>
			<Method name="SIM neg wet calibrant">
				<Mark main="Long Term" sub="Neg Wet"/>
			</Method>
			<Method name="SIM neg dry calibrant">
				<Mark main="Long Term" sub="Neg Dry"/>
			</Method>
		</Group>
		
		<Method name="source switch start pos"/>

		<Group repeat="10">
			<Method name="source switch neg wet from pos">
				<Mark main="Source Switch" sub="Neg Wet"/>
			</Method>
			<Method name="source switch neg dry from neg wet">
				<Mark main="Source Switch" sub="Neg Dry"/>
			</Method>
			<Method name="source switch pos from neg dry">
				<Mark main="Source Switch" sub="Pos Wet"/>
			</Method>
			<Method name="source switch neg dry from pos">
				<Mark main="Source Switch" sub="Neg Dry"/>
			</Method>
			<Method name="source switch neg wet from neg dry">
				<Mark main="Source Switch" sub="Neg Wet"/>
			</Method>
			<Method name="source switch pos from neg wet">
				<Mark main="Source Switch" sub="Pos Wet"/>
			</Method>
		</Group>

		<Group repeat="20">
			<Method name="SIM pos 3min soak">
				<Mark main="Short Term" sub="Pos Wet"/>
			</Method>
			<Method name="SIM neg wet 3min soak">
				<Mark main="Short Term" sub="Neg Wet"/>
			</Method>
			<Method name="SIM neg dry 3min soak">
				<Mark main="Short Term" sub="Neg Dry"/>
			</Method>
		</Group>

		<Method name="Source switching test Ultra DPIS" repeat="20">
			<Mark main="Source Switch Settle" sub="Three Phase"/>
		</Method>
		<Method name="Ultra +- performance check">
			<Mark main="Performance Check" sub="Three Phase"/>
		</Method>
		<Method name="Syft (+-) Standard Validation Ultra" repeat="3">
			<Mark main="SSV" sub="Three Phase"/>
		</Method>
	</Methods>

	<Charts>
		<Chart name="Sensitiveties and Impurities">
			<SubChart name="Sensitiveties" phase="Sample" accept="Above">
				<Mark main="Performance Check" sub="Three Phase"/>
				<Components>
					<Component reagent="O-" product="183" min="2.4"/>
					<Component reagent="O-" product="233" min="0.3"/>
					<Component reagent="OH-" product="91" min="10.5"/>
					<Component reagent="OH-" product="233" min="3.75"/>
					<Component reagent="O2-" product="124" min="3"/>
					<Component reagent="O2-" product="236" min="0.3"/>
					<Component reagent="H3O+" product="93" min="30"/>
					<Component reagent="NO+" product="92" min="45"/>
					<Component reagent="O2+" product="92" min="30"/>
					<Component reagent="O2+" product="236" min="15"/>
				</Components>
			</SubChart>
			<SubChart name="Impurities" phase="Background" accept="Under">
				<Mark main="Performance Check" sub="Three Phase"/>
				<Components>
					<Component reagent="O-" product="17" max="0.05"/>
					<Component reagent="O-" product="32" max="0.05"/>
					<Component reagent="OH-" product="16" max="0.05"/>
					<Component reagent="OH-" product="32" max="0.05"/>
					<Component reagent="O2-" product="17" max="0.05"/>
					<Component reagent="NO2-" product="16" max="0.05"/>
					<Component reagent="NO2-" product="62" max="0.05"/>
					<Component reagent="NO3-" product="46" max="0.05"/>
					<Component reagent="H3O+" product="18" max="0.05"/>
					<Component reagent="H3O+" product="28" max="0.05"/>
					<Component reagent="H3O+" product="29" max="0.05"/>
					<Component reagent="H3O+" product="32" max="0.05"/>
					<Component reagent="H3O+" product="57" max="0.05"/>
					<Component reagent="NO+" product="32" max="0.05"/>
					<Component reagent="O2+" product="29" max="0.05"/>
					<Component reagent="O2+" product="30" max="0.05"/>
					<Component reagent="O2+" product="56" max="0.05"/>
				</Components>
			</SubChart>
		</Chart>

		<Chart name="Long Term Stability">
			<SubChart name="Reagent Ions RSD" phase="Sample" accept="Under">
				<Mark main="Long Term" sub="Pos Wet"/>
				<Mark main="Long Term" sub="Neg Wet"/>
