					<Component reagent="NO2-" product="46"/>
					<Component reagent="NO3-" product="62"/>
				</Components>
			</SubChart>
		</Chart>

		<Chart name="Source Switch and Settle Time" >
			<SubChart name="Source Switch and Settle Time">
				<Mark main="Source Switch Settle" sub="Three Phase"/>
				<Components>
					<Component reagent="O-" product="16"/>
					<Component reagent="OH-" product="17"/>
					<Component reagent="O2-" product="32"/>
					<Component reagent="NO2-" product="46"/>
					<Component reagent="NO3-" product="62"/>
					<Component reagent="H3O+" product="19"/>
					<Component reagent="NO+" product="30"/>
					<Component reagent="O2+" product="32"/>
				</Components>
			</SubChart>
		</Chart>

		<Chart name="Injection Scan">
			<SubChart name="PosWet">
				<Mark main="Injection" sub="Pos Wet"/>
			</SubChart>
			<SubChart name="NegWet">
				<Mark main="Injection" sub="Neg Wet"/>
			</SubChart>
			<SubChart name="NegDry">
				<Mark main="Injection" sub="Neg Dry"/>
			</SubChart>
		</Chart>

		<Chart name="ICF">
			<SubChart name="ICF">
				<Mark main="SSV" sub="Three Phase"/>
			</SubChart>
		</Chart>
	</Charts>
</Config>