		<Method name="Ups scanline calibration"/>
		<Method name="Dws scanline calibration"/>
		<Method name="Detector Linearity Validation"/>
		<Method name="Syft (+-) Standard Validation"/>
		<Method name="Validation injection scan Pos"/>
		<Method name="Validation injection scan Neg Wet"/>
		<Method name="Validation injection scan Neg Dry"/>
