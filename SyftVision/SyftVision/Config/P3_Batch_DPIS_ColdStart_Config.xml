<?xml version="1.0" encoding="utf-8"?>
<Config>
	<Methods>
		<!--validation-->
		<Method name="Reset Quadrupoles"/>
		<Method name="carrier gas"/>
		<Method name="dws pressure validation"/>
		<Method name="Sample Flows"/>
		<Method name="ups scanline calibration"/>
		<Method name="dws scanline calibration"/>
		<Method name="detector verification"/>
		<Method name="Detector Linearity Validation"/>
		<Method name="Syft (+-) Standard Validation Ultra"/>
		<Method name="Background"/>
		<!--performance-->
		<Method name="Ultra +- performance check">
			<Mark main="Cold Start" sub="Performance"/>
		</Method>
		<!--first check-->
		<Method name="calibrant product settle test (PortOpen)">
			<Mark main="Cold Start" sub="Calibrant"/>
			<Mark main="Cold Start" sub="Both Ends Mark"/>
		</Method>
		<!--5*10 check-->
		<Group repeat="5">
			<Method name="SF+Linearity_Measure.js"/>
			<Method name="calibrant product settle test (PortOpen)" repeat="10">
				<Mark main="Cold Start" sub="Calibrant"/>
			</Method>
		</Group>
		<!--3hrs mark-->
		<Method name="SF+Linearity_Measure.js"/>
		<Method name="calibrant product settle test (PortOpen)" repeat="9">
			<Mark main="Cold Start" sub="Calibrant"/>
		</Method>
		<Method name="calibrant product settle test (PortOpen)">
			<Mark main="Cold Start" sub="Calibrant"/>
			<Mark main="Cold Start" sub="3hr And 4hr Mark"/>
		</Method>
		<!--10 check-->
		<Method name="SF+Linearity_Measure.js"/>
		<Method name="calibrant product settle test (PortOpen)" repeat="10">
			<Mark main="Cold Start" sub="Calibrant"/>
		</Method>
		<!--4hrs mark-->
		<Method name="SF+Linearity_Measure.js"/>
		<Method name="calibrant product settle test (PortOpen)" repeat="9">
			<Mark main="Cold Start" sub="Calibrant"/>
		</Method>
		<Method name="calibrant product settle test (PortOpen)">
			<Mark main="Cold Start" sub="Calibrant"/>
			<Mark main="Cold Start" sub="3hr And 4hr Mark"/>
			<Mark main="Cold Start" sub="Both Ends Mark"/>
		</Method>
		<!--validation-->
		<Method name="Reset Quadrupoles"/>
		<Method name="carrier gas"/>
		<Method name="dws pressure validation"/>
		<Method name="Sample Flows"/>
		<Method name="ups scanline calibration"/>
		<Method name="dws scanline calibration"/>
		<Method name="detector verification"/>
		<Method name="Detector Linearity Validation"/>
		<Method name="Syft (+-) Standard Validation Ultra"/>
		<Method name="Background"/>
		<!--performance-->
		<Method name="Ultra +- performance check">
			<Mark main="Cold Start" sub="Performance"/>
		</Method>
	</Methods>

	<Charts>
		<Chart name="Cold Start">
			<SubChart name="Sensitiveties" phase="Sample" accept="Above">
				<Mark main="Cold Start" sub="Performance"/>
				<Components>
					<Component reagent="O-" product="183" min="2.4"/>
					<Component reagent="O-" product="233" min="0.3"/>
					<Component reagent="OH-" product="91" min="10.5"/>
					<Component reagent="OH-" product="233" min="3.75"/>
					<Component reagent="O2-" product="124" min="3"/>
					<Component reagent="O2-" product="236" min="0.3"/>
					<Component reagent="H3O+" product="93" min="30"/>
					<Component reagent="NO+" product="92" min="45"/>
					<Component reagent="O2+" product="92" min="30"/>
					<Component reagent="O2+" product="236" min="15"/>
				</Components>
			</SubChart>
			<SubChart name="Sensitiveties Deviation" phase="Sample" accept="Under">
				<Mark main="Cold Start" sub="Performance"/>
				<Components>
					<Component reagent="O-" product="183" max="0.15"/>
					<Component reagent="O-" product="233" max="0.15"/>
					<Component reagent="OH-" product="91" max="0.15"/>
					<Component reagent="OH-" product="233" max="0.15"/>
					<Component reagent="O2-" product="124" max="0.15"/>
					<Component reagent="O2-" product="236" max="0.15"/>
					<Component reagent="H3O+" product="93" max="0.15"/>
					<Component reagent="NO+" product="92" max="0.15"/>
					<Component reagent="O2+" product="92" max="0.15"/>
					<Component reagent="O2+" product="236" max="0.15"/>
				</Components>
			</SubChart>
			<SubChart name="Reagent Ions" phase="Sample">
				<Mark main="Cold Start" sub="Calibrant"/>
				<Components>
					<Component reagent="O-" product="16"/>
					<Component reagent="OH-" product="17"/>
					<Component reagent="O2-" product="32"/>
					<Component reagent="NO2-" product="46"/>
					<Component reagent="NO3-" product="62"/>
					<Component reagent="H3O+" product="19"/>
					<Component reagent="NO+" product="30"/>
					<Component reagent="O2+" product="32"/>
				</Components>
			</SubChart>
			<SubChart name="3hr And 4hr Mark Deviation" phase="Sample" accept="Under">
				<Mark main="Cold Start" sub="3hr And 4hr Mark"/>
				<Components>
					<Component reagent="O-" product="16" max="0.1"/>
					<Component reagent="OH-" product="17" max="0.1"/>
					<Component reagent="O2-" product="32" max="0.1"/>
					<Component reagent="NO2-" product="46" max="0.1"/>
					<Component reagent="NO3-" product="62" max="0.1"/>
					<Component reagent="H3O+" product="19" max="0.1"/>
					<Component reagent="NO+" product="30" max="0.1"/>
					<Component reagent="O2+" product="32" max="0.1"/>
				</Components>
			</SubChart>
			<SubChart name="Both Ends Mark Deviation" phase="Sample">
				<Mark main="Cold Start" sub="Both Ends Mark"/>
				<Components>
					<Component reagent="O-" product="16"/>
					<Component reagent="OH-" product="17"/>
					<Component reagent="O2-" product="32"/>
					<Component reagent="NO2-" product="46"/>
					<Component reagent="NO3-" product="62"/>
					<Component reagent="H3O+" product="19"/>
					<Component reagent="NO+" product="30"/>
					<Component reagent="O2+" product="32"/>
				</Components>
			</SubChart>
		</Chart>
	</Charts>
</Config>
