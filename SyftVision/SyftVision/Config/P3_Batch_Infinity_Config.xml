<?xml version="1.0" encoding="utf-8" ?>
<Config>
	<Methods>
		<Method name="Reset Quadrupoles"/>
		<Method name="Ups scanline calibration"/>
		<Method name="Dws scanline calibration"/>
		<Method name="Detector Linearity Validation"/>
		<Method name="Syft (+-) Standard Validation">
			<Mark main="SSV" sub="Three Phase"/>
		</Method>
		<Method name="Validation injection scan Pos">
			<Mark main="Injection" sub="Pos Wet"/>
		</Method>
		<Method name="Validation injection scan Neg Wet">
			<Mark main="Injection" sub="Neg Wet"/>
		</Method>
		<Method name="Validation injection scan Neg Dry">
			<Mark main="Injection" sub="Neg Dry"/>
		</Method>
		<Method name="Validation Mass Scan Syft Standard (+-)"/>
		<Method name="Background"/>
		<Method name="infinity +- performance check_V5">
			<Mark main="Performance Check" sub="Three Phase"/>
		</Method>
		<Method name="Buffer SIM calLowConc 1min (PortOpen)"/>
		<Method name="calLowConc product settle test (PortOpen)" repeat="100">
			<Mark main="Stability" sub="Three Phase"/>
		</Method>
		<Method name="Buffer SIM calLowConc 1min (PortOpen)"/>

		<Group repeat="10">
			<Method name="Ups scanline calibration"/>
			<Method name="Dws scanline calibration"/>
			<Method name="Syft (+-) Standard Validation">
				<Mark main="SSV" sub="Three Phase"/>
			</Method>
			<Method name="Infinity SIM calLowConc 4min" repeat="3">
				<Mark main="EOV" sub="Three Phase"/>
			</Method>
		</Group>
		
		<Method name="SIM pos calLowConc"/>
		<Method name="SIM neg wet calLowConc"/>
		<Method name="SIM neg dry calLowConc"/>
		<Method name="Buffer SIM diluent 1min (PortOpen)"/>
		<Method name="LOD SIM v8_Flushing" repeat="20"/>
		<Method name="LOD SIM v8" repeat="12">
			<Mark main="LODs" sub="Three Phase"/>
		</Method>

		<Group repeat="2">
			<Method name="LOD SIM Background - 75 Compounds">
				<Mark main="Background" sub="75 Compounds"/>
			</Method>
			<Method name="Samsung 2020_1 51com_v3" >
				<Mark main="Background" sub="51 Compounds"/>
			</Method>
			<Method name="Samsung 2020_2 52com">
				<Mark main="Background" sub="52 Compounds" />
			</Method>
		</Group>
		
		<Method name="Buffer SIM calLowConc 1min (PortOpen)"/>
		<Method name="infinity +- performance check_V5">
			<Mark main="Performance Check" sub="Three Phase"/>
		</Method>
	</Methods>

	<Charts>
		<Chart name="Sensitiveties and Impurities" >
			<SubChart name="Sensitiveties" phase="Sample" accept="Above">
				<Mark main="Performance Check" sub="Three Phase"/>
				<Components>
					<Component reagent="O-" product="183" min="80" />
					<Component reagent="OH-" product="233" min="150" />
					<Component reagent="H3O+" product="93" min="465"/>
					<Component reagent="NO+" product="92" min="321"/>
					<Component reagent="O2+" product="28" min="55"/>
					<Component reagent="O2+" product="92" min="300"/>
					<Component reagent="O2+" product="236" min="330"/>
				</Components>
			</SubChart>
			<SubChart name="Impurities" phase="Background" accept="Under">
				<Mark main="Performance Check" sub="Three Phase"/>
				<Components>
					<Component reagent="O-" product="17" max="0.01" />
					<Component reagent="OH-" product="16" max="0.05" />
					<Component reagent="OH-" product="19" max="0.01" />
					<Component reagent="O2-" product="16" max="0.01" />
					<Component reagent="NO2-" product="16" max="0.01" />
					<Component reagent="H3O+" product="29" max="0.09" />
					<Component reagent="H3O+" product="32" max="0.05" />
					<Component reagent="H3O+" product="57" max="0.09" />
					<Component reagent="NO+" product="32" max="0.05" />
					<Component reagent="O2+" product="19" max="0.01" />
					<Component reagent="O2+" product="30" max="0.04" />
					<Component reagent="O2+" product="56" max="0.02" />
				</Components>
			</SubChart>
		</Chart>

		<Chart name="Stability">
			<SubChart name="Concentrations RSD" phase="Sample" accept="Under">
				<Mark main="Stability" sub="Three Phase"/>
				<Components>
					<Component reagent="O-" product="19" max="0.05" />
					<Component reagent="O-" product="183" max="0.05" />
					<Component reagent="OH-" product="19" max="0.1" />
					<Component reagent="OH-" product="183" max="0.05" />
					<Component reagent="OH-" product="233" max="0.05" />
					<Component reagent="O2-" product="236" max="0.05" />
					<Component reagent="H3O+" product="93" max="0.05" />
					<Component reagent="NO+" product="92" max="0.05" />
					<Component reagent="O2+" product="28" max="0.05" />
					<Component reagent="O2+" product="92" max="0.05" />
					<Component reagent="O2+" product="236" max="0.05" />
				</Components>
			</SubChart>
			<SubChart name="Concentrations" phase="Sample">
				<Mark main="Stability" sub="Three Phase"/>
				<Components>
					<Component reagent="O-" product="19"/>
					<Component reagent="O-" product="183"/>
					<Component reagent="OH-" product="19"/>
					<Component reagent="OH-" product="183"/>
					<Component reagent="OH-" product="233"/>
					<Component reagent="H3O+" product="93"/>
					<Component reagent="NO+" product="92"/>
					<Component reagent="O2+" product="28"/>
					<Component reagent="O2+" product="92"/>
					<Component reagent="O2+" product="236"/>
				</Components>
			</SubChart>
			<SubChart name="Reagent Ions">
				<Mark main="Stability" sub="Three Phase"/>
				<Components>
					<Component reagent="O-" product="16"/>
					<Component reagent="OH-" product="17"/>
					<Component reagent="O2-" product="32"/>
					<Component reagent="NO2-" product="46"/>
					<Component reagent="H3O+" product="19"/>
					<Component reagent="NO+" product="30"/>
					<Component reagent="O2+" product="32"/>
				</Components>
			</SubChart>
			<SubChart name="Product Ions">
				<Mark main="Stability" sub="Three Phase"/>
				<Components>
					<Component reagent="O-" product="183"/>
					<Component reagent="OH-" product="91"/>
					<Component reagent="OH-" product="149"/>
					<Component reagent="OH-" product="183"/>
					<Component reagent="OH-" product="233"/>
					<Component reagent="O2-" product="236"/>
					<Component reagent="H3O+" product="79"/>
					<Component reagent="H3O+" product="93"/>
					<Component reagent="NO+" product="78"/>
					<Component reagent="NO+" product="92"/>
					<Component reagent="NO+" product="150"/>
					<Component reagent="O2+" product="28"/>
					<Component reagent="O2+" product="92"/>
					<Component reagent="O2+" product="150"/>
					<Component reagent="O2+" product="186"/>
					<Component reagent="O2+" product="236"/>
				</Components>
			</SubChart>
			<SubChart name="Concentrations wEOV" phase="Sample">
				<Mark main="EOV" sub="Three Phase"/>
				<Components>
					<Component reagent="O-" product="183"/>
					<Component reagent="OH-" product="183"/>
					<Component reagent="OH-" product="233"/>
					<Component reagent="H3O+" product="93"/>
					<Component reagent="NO+" product="92"/>
					<Component reagent="O2+" product="28"/>
					<Component reagent="O2+" product="92"/>
					<Component reagent="O2+" product="236"/>
				</Components>
			</SubChart>
		</Chart>

		<Chart name="LODs">
			<SubChart name="LODs" phase="Sample" accept="Under">
				<Mark main="LODs" sub="Three Phase"/>
				<Components>
					<Component reagent="OH-" product="19" max="0.25"/>
					<Component reagent="OH-" product="33" max="0.05"/>
					<Component reagent="OH-" product="46" max="0.05"/>
					<Component reagent="O2-" product="64" max="0.1"/>
					<Component reagent="NO2-" product="37" max="0.05"/>
					<Component reagent="NO+" product="59" max="0.05"/>
					<Component reagent="NO+" product="93" max="0.05"/>
					<Component reagent="NO+" product="131" max="0.02"/>
					<Component reagent="O2+" product="17" max="0.5"/>
					<Component reagent="O2+" product="92" max="0.05"/>
				</Components>
			</SubChart>
		</Chart>

		<!--Components = Compounds, reagent = compound, max (ppb) = limit of mean of analyte concentration, min (ppb) = limit of lod of analyte concentration -->
		<Chart name="Background">
			<SubChart name="75 Compounds" phase="Sample">
				<!--default max = 1, default min = 0.1-->
				<Mark main="Background" sub="75 Compounds"/>
				<Components>
					<Component reagent="acetaldehyde" max="5" min="1"/>
					<Component reagent="acetic acid" max="1" min="1"/>
					<Component reagent="cyclohexane" max="1" min="1"/>
					<Component reagent="dibutyl phthalate" max="5" min="1"/>
					<Component reagent="dichloromethane" max="1" min="1"/>
					<Component reagent="dimethylamine" max="1" min="1"/>
					<Component reagent="epichlorohydrin" max="1" min="1"/>
					<Component reagent="ethanol + dimethyl ether" max="1" min="1"/>
					<Component reagent="formaldehyde" max="1" min="1"/>
					<Component reagent="hexane" max="1" min="1"/>
					<Component reagent="hydrogen sulfide" max="1" min="1"/>
					<Component reagent="methanol" max="5" min="1"/>
					<Component reagent="methylamine" max="5" min="1"/>
					<Component reagent="perfluorohexane" max="1" min="1"/>
					<Component reagent="toluene" max="1" min="1"/>
					<Component reagent="xylenes + ethylbenzene" max="1" min="1"/>
				</Components>
			</SubChart>
			<SubChart name="51 Compounds" phase="Sample">
				<!--default max = 2, default min = 0.5-->
				<Mark main="Background" sub="51 Compounds"/>
				<Components>
					<Component reagent="dimethylamine" max="12" min="1.5"/>
					<Component reagent="ethanol + dimethyl ether + formic acid" max="12" min="1.5"/>
					<Component reagent="formaldehyde" max="12" min="1.5"/>
					<Component reagent="methanol" max="12" min="1.5"/>
					<Component reagent="perfluorotributylamine" max="0.3" min="0.1"/>
					<Component reagent="PFC" max="0.3" min="0.1"/>
					<Component reagent="propylene glycol methyl ether acetate" max="0.3" min="0.1"/>
					<Component reagent="Tetraethoxysilane(TEOS)" max="0.3" min="0.1"/>
					<Component reagent="tricresyl phosphate" max="0.3" min="0.1"/>
					<Component reagent="triethylborate" max="0.3" min="0.1"/>
					<Component reagent="triethylphosphate" max="0.3" min="0.1"/>
					<Component reagent="trimethylsilanol" max="0.3" min="0.1"/>
				</Components>
			</SubChart>
			<SubChart name="52 Compounds" phase="Sample">
				<!--default max = 2, default min = 0.5-->
				<Mark main="Background" sub="52 Compounds"/>
				<Components>
					<Component reagent="acetaldehyde" max="12" min="1.5"/>
					<Component reagent="ethanol + dimethyl ether + formic acid" max="12" min="1.5"/>
					<Component reagent="hexane" max="12" min="1.5"/>
					<Component reagent="methylamine" max="12" min="1.5"/>
				</Components>
			</SubChart>
		</Chart>

		<Chart name="Injection Scan">
			<SubChart name="PosWet">
				<Mark main="Injection" sub="Pos Wet"/>
			</SubChart>
			<SubChart name="NegWet">
				<Mark main="Injection" sub="Neg Wet"/>
			</SubChart>
			<SubChart name="NegDry">
				<Mark main="Injection" sub="Neg Dry"/>
			</SubChart>
		</Chart>

		<Chart name="ICF">
			<SubChart name="ICF">
				<Mark main="SSV" sub="Three Phase"/>
			</SubChart>
		</Chart>
	</Charts>
</Config>
